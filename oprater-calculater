username = input("Enter ur number ")
username2 = input("Enter ur 2nd number ")
print("for pluse",int(username) + int(username2))
print("for minus", int(username) - int(username2))
  
print("for divide ",int(username) / int(username2))
print("for multiply",int(username) * int(username2))
print(" for moduklke",int(username) % int(username2))
