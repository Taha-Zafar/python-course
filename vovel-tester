x=(input("enter your number "))
match x:
    case 'a':
         print("this is a vovel")
    case 'A' :
         print("this is a vovel")
    case 'e' :
         print("this is a vovel")      
    case 'E' :
         print("this is a vovel")
    case 'i':
         print("this is a vovel")
    case 'I' :
         print("this is a vovel")
    case 'o' :
         print("this is a vovel")      
    case 'O' :
         print("this is a vovel")
    case _:
         print("This is not a vovle")
